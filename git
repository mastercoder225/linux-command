# 配置用户名和邮箱
git config --global user.name "Your Name"
git config --global user.email "your_email@example.com"

# 配置文本编辑器
git config --global core.editor vim

# 配置常用的别名
git config --global alias.co checkout
git config --global alias.br branch
git config --global alias.ci commit
git config --global alias.st status

# 初始化仓库
git init

# 克隆仓库
git clone git@github.com:mastercoder225/linux-command.git
git clone git@github.com:mastercoder225/linux-command.git linux-command-copy

# 跟踪新文件
git add NewFile.txt

# 暂存已修改文件
git add ModifiedFile.txt

# 标记冲突文件为已解决状态
git add ConflictFile.txt

# 提交更新
git commit
git commit -m "fix"
git commit -a -m "fix"

# 移除文件
git rm FileToRemove.txt
git rm FileToRemove.txt -f
git rm FileToRemove.txt --cached



git config --list
git config user.name

# git help
git help branch

# git commit
git add file.txt
git commit -m "add file.txt"

# git status
git status
git status -s

# git remote
git remote
git remote -v
git remote show origin

git remote add origin git@github.com:mastercoder225/linux-command.git

git fetch origin
git push origin master

# git alias


# git branch
git branch
git branch -v
git branch -vv
git branch --merged
git branch --no-merged
git checkout -b bugfix
git checkout --track origin/master
git checkout -b bugfix origin/master
git push origin --delete bugfix
git checkout master
git merge bugfix
git branch -d bugfix

# git rebase

